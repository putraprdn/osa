{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"esModuleInterop": true,
		"declaration": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"strictBindCallApply": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitThis": true,
		"alwaysStrict": true
	},
	"exclude": ["node_modules"]
}
